{
  "name": "web",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "dotenvx run --strict -f .env.development --env-keys-file .env.keys --overload --debug -- next dev --turbopack --port 3000",
    "build": "dotenvx run --strict -f .env.production --env-keys-file .env.keys --overload --debug -- next build",
    "start": "dotenvx run --strict -f .env.production --env-keys-file .env.keys --overload --debug -- next start",
    "lint": "next lint --max-warnings 0",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.45.1",
    "@repo/ui": "*",
    "next": "^15.3.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/node": "^22.15.3",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "eslint": "^9.29.0",
    "typescript": "5.8.2"
  }
}
